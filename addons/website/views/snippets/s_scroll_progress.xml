</record>

<record id="website.s_scroll_progress_000_js" model="ir.asset">
    <field name="name">Scroll Progress 000 JS</field>
    <field name="bundle">web.assets_frontend</field>
    <field name="path">website/static/src/snippets/s_scroll_progress/000.js</field>
</record>

</odoo>
