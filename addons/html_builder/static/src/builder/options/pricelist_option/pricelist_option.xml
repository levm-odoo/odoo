<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

<t t-name="html_builder.AddProductButton">
    <BuilderRow label.translate="Product">
        <BuilderButtonGroup>
            <BuilderButton
                action="'addItem'"
                actionParam="`${props.productSelector}:last-of-type`"
                preview="false"
                className="'o_we_bg_brand_primary'"
                applyTo="props.applyTo">
                Add Product
            </BuilderButton>
        </BuilderButtonGroup>
    </BuilderRow>
</t>

</templates>
